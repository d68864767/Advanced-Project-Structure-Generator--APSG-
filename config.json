{
  "project": {
    "name": "advanced-project-structure-generator",
    "version": "1.0.0",
    "description": "A sophisticated Node.js application designed to create, manage, and optimize large-scale, complex project directory trees."
  },
  "server": {
    "port": 3000
  },
  "database": {
    "host": "localhost",
    "port": 27017,
    "name": "apsg_db"
  },
  "optimization": {
    "enable": true,
    "rules": "rules.js"
  },
  "visualization": {
    "enable": true,
    "url": "http://localhost:3000"
  },
  "versionControl": {
    "type": "git",
    "repository": "https://github.com/yourusername/advanced-project-structure-generator.git"
  },
  "documentation": {
    "enable": true,
    "output": "README.md"
  },
  "analytics": {
    "enable": true
  },
  "cli": {
    "enable": true
  }
}
